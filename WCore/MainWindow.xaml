<Window x:Class="WCore.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WCore"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">

        <DataGrid x:Name="dataGrid" Margin="10,10,10,235"/>
        <Button x:Name="btn" Content="logon" HorizontalAlignment="Left" Margin="23,241,0,0" VerticalAlignment="Top" Click="btn_Click"/>
        <Button x:Name="btn_data" Content="fill data" HorizontalAlignment="Left" Margin="23,276,0,0" VerticalAlignment="Top" Click="btn_data_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="100,243,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="100,276,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="check config code" HorizontalAlignment="Left" Margin="23,311,0,0" VerticalAlignment="Top" Click="button_Click_1"/>


    </Grid>
</Window>
